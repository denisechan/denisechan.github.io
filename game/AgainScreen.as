package  {		import flash.display.MovieClip;	import flash.events.MouseEvent;		public class AgainScreen extends MovieClip {						public function AgainScreen() {			playAgain.addEventListener(MouseEvent.CLICK, playAgainClicked);		}				private function playAgainClicked (evt:MouseEvent) {			var titleScreen:MovieClip = new TitleScreen();			parent.addChild(titleScreen);			parent.removeChild(this);					}	}	}