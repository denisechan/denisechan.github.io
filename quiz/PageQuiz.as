package  {		import flash.display.MovieClip;	import flash.events.MouseEvent;		public class PageQuiz extends MovieClip {		var titleScreen: TitleScreen;		var questionOne: QuestionOne;		var questionTwo: QuestionTwo;		var questionThree: QuestionThree;		var questionFour: QuestionFour;		var questionFive: QuestionFive;		var resultScreen: ResultScreen;		var gotoTwoScreen: GotoTwoScreen;		var gotoThreeScreen: GotoThreeScreen;		var gotoFourScreen: GotoFourScreen;		var gotoFiveScreen: GotoFiveScreen;		var gotoResultScreen: GotoResultScreen;		var score:int = 0;				public function PageQuiz() {			titleScreen = new TitleScreen();			addChild(titleScreen);			titleScreen.goButton.addEventListener(MouseEvent.CLICK, onGoButtonClicked);								}		//go button on the home page		function onGoButtonClicked(evt:MouseEvent) {			questionOne = new QuestionOne();			removeChild(titleScreen);			addChild(questionOne);			questionOne.one1Button.addEventListener(MouseEvent.CLICK, one1ButtonClicked);			questionOne.one2Button.addEventListener(MouseEvent.CLICK, one2ButtonClicked);			questionOne.one3Button.addEventListener(MouseEvent.CLICK, one3ButtonClicked);			score = 0;		}		//question one option one		function one1ButtonClicked(evt:MouseEvent) {			gotoTwoScreen = new GotoTwoScreen();			addChild(gotoTwoScreen);			questionOne.one1Button.mouseEnabled = false;			questionOne.one2Button.mouseEnabled = false;			questionOne.one3Button.mouseEnabled = false;			gotoTwoScreen.responseText.text = "Incorrect! There are FOUR big parts in a brain!";			gotoTwoScreen.gotoTwo.addEventListener(MouseEvent.CLICK, gotoTwoClicked);					}		//question one option two		function one2ButtonClicked(evt:MouseEvent) {			gotoTwoScreen = new GotoTwoScreen();			addChild(gotoTwoScreen);			gotoTwoScreen.responseText.text = "Incorrect! There are FOUR big parts in a brain!";			questionOne.one1Button.mouseEnabled = false;			questionOne.one2Button.mouseEnabled = false;			questionOne.one3Button.mouseEnabled = false;			gotoTwoScreen.gotoTwo.addEventListener(MouseEvent.CLICK, gotoTwoClicked);		}		//question one option three		function one3ButtonClicked(evt:MouseEvent) {			gotoTwoScreen = new GotoTwoScreen();			addChild(gotoTwoScreen);			gotoTwoScreen.responseText.text = "Correct! There are FOUR big parts in a brain!";			questionOne.one1Button.mouseEnabled = false;			questionOne.one2Button.mouseEnabled = false;			questionOne.one3Button.mouseEnabled = false;			score++;			gotoTwoScreen.gotoTwo.addEventListener(MouseEvent.CLICK, gotoTwoClicked);		}				//go to number two		function gotoTwoClicked(evt:MouseEvent) {			questionTwo = new QuestionTwo();			addChild(questionTwo);			removeChild(questionOne);			removeChild(gotoTwoScreen);			questionTwo.two1Button.addEventListener(MouseEvent.CLICK, two1ButtonClicked);			questionTwo.two2Button.addEventListener(MouseEvent.CLICK, two2ButtonClicked);		}				//question two option one		function two1ButtonClicked(evt:MouseEvent) {			gotoThreeScreen = new GotoThreeScreen();			addChild(gotoThreeScreen);			gotoThreeScreen.responseText.text = "That's right!"			questionTwo.two1Button.mouseEnabled = false;			questionTwo.two2Button.mouseEnabled = false;			score++;			gotoThreeScreen.gotoThree.addEventListener(MouseEvent.CLICK, gotoThreeClicked);					}				//question two option two		function two2ButtonClicked(evt:MouseEvent) {			gotoThreeScreen = new GotoThreeScreen();			addChild(gotoThreeScreen);			gotoThreeScreen.responseText.text = "Oh no! This statement is true!"			questionTwo.two1Button.mouseEnabled = false;			questionTwo.two2Button.mouseEnabled = false;			gotoThreeScreen.gotoThree.addEventListener(MouseEvent.CLICK, gotoThreeClicked);		}				//go to number three		function gotoThreeClicked(evt:MouseEvent) {			questionThree = new QuestionThree();			addChild(questionThree);			removeChild(questionTwo);			removeChild(gotoThreeScreen);			questionThree.three1Button.addEventListener(MouseEvent.CLICK, three1ButtonClicked);			questionThree.three2Button.addEventListener(MouseEvent.CLICK, three2ButtonClicked);		}				//number three option one		function three1ButtonClicked(evt:MouseEvent) {			gotoFourScreen = new GotoFourScreen();			addChild(gotoFourScreen);			gotoFourScreen.responseText.text = "Not randomly! Feelings go wrong when they fill up too much!"			questionThree.three1Button.mouseEnabled = false;			questionThree.three2Button.mouseEnabled = false;			gotoFourScreen.gotoFour.addEventListener(MouseEvent.CLICK, gotoFourClicked);		}				//number three option two 		function three2ButtonClicked(evt:MouseEvent) {			gotoFourScreen = new GotoFourScreen();			addChild(gotoFourScreen);			gotoFourScreen.responseText.text = "Right! Feelings go wrong when they fill up too much!"			questionThree.three1Button.mouseEnabled = false;			questionThree.three2Button.mouseEnabled = false;			gotoFourScreen.gotoFour.addEventListener(MouseEvent.CLICK, gotoFourClicked);			score++;		}				//goto number four		function gotoFourClicked(evt:MouseEvent) {			questionFour = new QuestionFour();			removeChild(questionThree);			removeChild(gotoFourScreen);			addChild(questionFour);			questionFour.four1Button.addEventListener(MouseEvent.CLICK, four1ButtonClicked);			questionFour.four2Button.addEventListener(MouseEvent.CLICK, four2ButtonClicked);		}				//number four option one		function four1ButtonClicked(evt:MouseEvent) {			gotoFiveScreen = new GotoFiveScreen();			addChild(gotoFiveScreen);			gotoFiveScreen.responseText.text = "Incorrect! The mentally ill were treated as devils!"			questionFour.four1Button.mouseEnabled = false;			questionFour.four2Button.mouseEnabled = false;			gotoFiveScreen.gotoFive.addEventListener(MouseEvent.CLICK, gotoFiveClicked);		}				//number four option two		function four2ButtonClicked(evt:MouseEvent) {			gotoFiveScreen = new GotoFiveScreen();			addChild(gotoFiveScreen);			gotoFiveScreen.responseText.text = "Correct! The mentally ill were treated as devils!"			questionFour.four1Button.mouseEnabled = false;			questionFour.four2Button.mouseEnabled = false;			gotoFiveScreen.gotoFive.addEventListener(MouseEvent.CLICK, gotoFiveClicked);			score++;		}				//go to number five		function gotoFiveClicked(evt:MouseEvent) {			questionFive = new QuestionFive();			removeChild(questionFour);			removeChild(gotoFiveScreen);			addChild(questionFive);			questionFive.five1Button.addEventListener(MouseEvent.CLICK, five1ButtonClicked);			questionFive.five2Button.addEventListener(MouseEvent.CLICK, five2ButtonClicked);		}				//question five option one		function five1ButtonClicked(evt:MouseEvent) {			gotoResultScreen = new GotoResultScreen();			addChild(gotoResultScreen);			gotoResultScreen.responseText.text = "That is correct!"			questionFive.five1Button.mouseEnabled = false;			questionFive.five2Button.mouseEnabled = false;			gotoResultScreen.resultButton.addEventListener(MouseEvent.CLICK, resultButtonClicked);			score++;		}				//question five option two		function five2ButtonClicked(evt:MouseEvent) {			gotoResultScreen = new GotoResultScreen();			addChild(gotoResultScreen);			gotoResultScreen.responseText.text = "Oops! This statement is true!"			questionFive.five1Button.mouseEnabled = false;			questionFive.five2Button.mouseEnabled = false;			gotoResultScreen.resultButton.addEventListener(MouseEvent.CLICK, resultButtonClicked);		}				//go to result page		function resultButtonClicked(evt:MouseEvent) {			resultScreen = new ResultScreen();			addChild(resultScreen);			removeChild(questionFive);			removeChild(gotoResultScreen);			resultScreen.playAgain.addEventListener(MouseEvent.CLICK, playAgainClicked);			switch (score) {								case 0:				resultScreen.scoreText.text = "You got 0 out of 5, 0%";				resultScreen.messageText.text = "watch tutorials and try again!";				break;								case 1:				resultScreen.scoreText.text = "You got 1 out of 5, 20%";				resultScreen.messageText.text = "watch tutorials and try again!";				break;								case 2:				resultScreen.scoreText.text = "You got 2 out of 5, 40%!";				resultScreen.messageText.text = "OH NO! review and try again!";				break;								case 3:				resultScreen.scoreText.text = "You got 3 out of 5, 60%!";				resultScreen.messageText.text = "WORK HARD! you can do better!";				break;								case 4:				resultScreen.scoreText.text = "You got 4 out of 5, 80%!";				resultScreen.messageText.text = "GOOD JOB! That's almost a perfect!";				break;								case 5:				resultScreen.scoreText.text = "You got 5 out of 5, 100%!";				resultScreen.messageText.text = "WOW! excellent!";				break;			}		}				//play again		function playAgainClicked(evt:MouseEvent) {			removeChild(resultScreen);			addChild(titleScreen);		}							}	}